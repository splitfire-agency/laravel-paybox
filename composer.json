{
  "name": "splitfire-agency/laravel-paybox",
  "description": "Laravel 9 Paybox",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "laravel/framework": "^8.0|^9.0|^10.0",
    "ext-openssl": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "guzzlehttp/guzzle": ">=5.3"
  },
  "autoload": {
    "psr-4": {
      "Sf\\PayboxGateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.5.20",
    "mockery/mockery": "^1.4.0",
    "orchestra/testbench": "^7.5",
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sf\\PayboxGateway\\Providers\\PayboxServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
